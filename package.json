{
  "name": "vsl",
  "version": "0.0.2-alpha.3",
  "description": "VSL: Versatile Scripting Language",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prepare": "npm run build",
    "build": "npm run nearley && cross-env NODE_ENV=production babel src/ -d lib/",
    "dev": "npm run nearley && npm run babel",
    "babel": "babel src/ -d lib/ -s",
    "webpack": "webpack",
    "nearley": "mkdirp lib/vsl/parser && nearleyc src/vsl/parser/parser.ne -o src/vsl/parser/parser.js",
    "compileParser": "babel src/vsl/parser/parser.js -o lib/vsl/parser/parser.js",
    "parser": "npm run nearley && npm run compileParser",
    "watch": "npm run watchParser; babel src/ -d lib/ -s --watch",
    "watchParser": "node misc/parser-watcher.js &",
    "clean": "rm -r lib",
    "lint": "eslint src",
    "docs": "esdoc",
    "test": "npm run nearley && mocha",
    "coverage": "cross-env NODE_ENV=test nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsl-lang/VSL.git"
  },
  "bin": {
    "vsl": "lib/cli/vsl.js"
  },
  "author": "somebody1234",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vsl-lang/VSL/issues"
  },
  "homepage": "https://github.com/vsl-lang/VSL#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.34",
    "@babel/runtime": "^7.0.0-beta.34",
    "astring": "^1.2.0",
    "axios": "^0.18.0",
    "browser-process-hrtime": "^0.1.2",
    "colors": "^1.1.2",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "js-yaml": "^3.8.4",
    "llir": "0.0.1-beta.1",
    "llvm-node": "git+https://github.com/MichaReiser/llvm-node.git",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-contenteditable": "^2.0.2",
    "react-dom": "^15.6.1",
    "semver": "^5.3.0",
    "vsl-nearley": "^2.9.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.34",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.34",
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.34",
    "@babel/plugin-proposal-export-default": "^7.0.0-beta.32",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-beta.34",
    "@babel/plugin-proposal-function-bind": "^7.0.0-beta.34",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.34",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.34",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.34",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.34",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-include": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-wildcard": "^4.0.0",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "esdoc": "^1.0.3",
    "esdoc-babel-7-plugin": "^1.1.0",
    "esdoc-babel-plugin": "github:mhelvens/esdoc-babel-plugin",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.1",
    "esdoc-inject-script-plugin": "^1.0.0",
    "esdoc-inject-style-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^3.19.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "install": "^0.10.1",
    "mocha": "^3.5.0",
    "npm": "^5.3.0",
    "nyc": "^10.3.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/vsl/**/*.js"
    ],
    "exclude": [],
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "pkg": {
    "scripts": [
      "lib/**/*.js"
    ],
    "assets": [
      "libraries/libvsl-x/**/*"
    ]
  }
}
